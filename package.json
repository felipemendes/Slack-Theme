{
    "name": "slack-theme",
    "displayName": "Slack Theme",
    "description": "Color scheme inspired by Slack's default theme",
    "version": "1.5.1",
    "publisher": "felipe-mendes",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "slack",
        "theme",
        "light",
        "dark"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#FFF",
        "theme": "light"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/felipemendes/slack-theme"
    },
    "contributes": {
        "themes": [
            {
                "label": "Slack Theme",
                "uiTheme": "vs",
                "path": "./themes/slack-theme.json"
            },{
                "label": "Slack Theme Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/slack-theme-dark.json"
            }
        ]
